#include "stdio.h"
int main () {
    int num[10];
    int i = 0;
    int max = 0;
    int n = 0;
    printf(" Pls enter number of elements \n");
    scanf(" %d", &n);    
    
    printf(" Number of elements are : %d \n",n);
    
    for (i = 0; i < n; i ++){
        scanf(" %d", &num[i]);
        if (num[i] > max){
            max = num[i];
        }
    }
    printf(" Maximum number is : %d \n", max);

    return 0;
}
